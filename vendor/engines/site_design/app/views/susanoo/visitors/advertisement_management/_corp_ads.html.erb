<div class="row">
  <h4><%= t('advertisement_management.susanoo.visitors.corp_title') %></h4>
  <ul class="thumbnails">
    <% @corp_ads.each do |ad| -%>
      <li class="span3">
        <%= link_to ad.url, class: "thumbnail", target: '_blank' do %>
          <%= image_tag ad_image_path(ad.image), alt: ad.alt %>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
