.yui-calcontainer{position:relative;width:14em;float:none;font-size:14px}
.yui-skin-sam .yui-calcontainer .title{background:url(<%= asset_path("event_calendar/sprite.png") %>) repeat-x 0 0;border-bottom:1px solid #ccc;font:100% sans-serif;color:#000;font-weight:bold;height:auto;padding:.4em;margin:0 -10px 10px -10px;top:0;left:0;text-align:left;}
.yui-skin-sam .yui-calcontainer .calclose{background:url(<%= asset_path("event_calendar/sprite.png") %>) no-repeat 0 -300px;width:25px;height:15px;top:.4em;right:.4em;cursor:pointer;}
.yui-skin-sam .yui-calendar .calnavleft{background:url(<%= asset_path("event_calendar/sprite.png") %>) no-repeat 0 -450px;width:25px;height:15px;top:0;bottom:0;left:-10px;margin-left:.4em;cursor:pointer;}
.yui-skin-sam .yui-calendar .calnavright{background:url(<%= asset_path("event_calendar/sprite.png") %>) no-repeat 0 -500px;width:25px;height:15px;top:0;bottom:0;right:-10px;margin-right:.4em;cursor:pointer;}
.yui-skin-sam .yui-calendar .calrowfoot{text-align:left;padding:0 0 0 2px;}
.yui-skin-sam .yui-calendar td.calcell.restricted{text-decoration:none}
.yui-skin-sam .yui-calendar a.calnav:hover{background:url(<%= asset_path("event_calendar/sprite.png") %>) repeat-x 0 0;border-color:#A0A0A0;cursor:pointer;}
.yui-skin-sam .yui-calcontainer .yui-cal-nav .yui-cal-nav-btn{border:1px solid #808080;background:url(<%= asset_path("event_calendar/sprite.png") %>) repeat-x 0 0;background-color:#ccc;margin:auto .15em;}
.yui-skin-sam .yui-calcontainer .yui-cal-nav .yui-cal-nav-btn.yui-default{border:1px solid #304369;background-color:#426fd9;background:url(<%= asset_path("event_calendar/sprite.png") %>) repeat-x 0 -1400px;}
.yui-skin-sam .yui-calendar td.calcell.selectable{font-weight:bold;}
.yui-skin-sam .yui-calendar td.calcell a{text-decoration:underline;}